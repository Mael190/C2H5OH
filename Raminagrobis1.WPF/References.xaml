<Page x:Class="Raminagrobis1.WPF.References"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Raminagrobis1.WPF"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="References">

    <Grid Background="White">
        <TextBlock Text="Gestion des références" HorizontalAlignment="Center" Margin="0,10,0,-10" />
        <StackPanel Margin="10,104,10,-24">
            <DockPanel Margin="10">
                <WrapPanel HorizontalAlignment="Center" DockPanel.Dock="Top" Margin="0,0,0,10">
                    <Button x:Name="btnOpenFile" Click="btnOpenFile_Click" Content="Open file"/>
                </WrapPanel>
            </DockPanel>
            <ComboBox x:Name="listFournisseurs" DisplayMemberPath="Societe" Width="92"/>
        </StackPanel>
        <TextBlock HorizontalAlignment="Center" Margin="0,41,0,-41" FontWeight="Normal" ><Run Language="fr-fr" Text="Choisissez un fournisseur et importer un fichier CSV "/></TextBlock>
    </Grid>
</Page>
